                <link rel="stylesheet" href="/pos_custom/static/src/css/jquery-ui.css" />
                <link rel="stylesheet" href="/pos_custom/static/src/css/pos_custom.css" />
            </xpath>
    	    <xpath expr="//script[@id='loading-script']" position="before">
                <script type="text/javascript" src="/pos_custom/static/src/js/jquery-ui.js"></script>
                <script type="text/javascript" src="/pos_custom/static/src/js/pos_custom.js"></script>
    	    </xpath>
        </template>

    </data>
</openerp>
